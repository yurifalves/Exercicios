algoritmo sequenciaCaract;
// Síntese: Avalia 2 sequências de caracteres
//  Objetivo:  
//  Entrada :
//  Saída   :


principal
	caracter vetor1[100], vetor2[100];
	inteiro N;



	inteiro i;
	leia(N);
	para (i de 0 ate N-1 passo 1) faca
		leiaCaracter(vetor1[i]);
		leiaCaracter(vetor2[i]);
	fimPara

	se (compara_sequencias(vetor1, vetor2, N)==0) entao escreval("As sequencias sao diferentes!");
	senao escreval("As sequencias sao iguais!"); fimSe

fimPrincipal

funcao inteiro compara_sequencias(caracter vetor1[], caracter vetor2[], inteiro N)
	inteiro v;

	inteiro i;
	v = 0;
	para (i de 0 ate N-1 passo 1) faca
		se (vetor1[i]==vetor2[i]) entao v = v + 1; fimSe
	fimPara

	se (v==N) entao retorna 1; senao retorna 0; fimSe
fimFuncao
