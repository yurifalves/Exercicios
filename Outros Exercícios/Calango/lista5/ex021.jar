algoritmo resistencia;
// Síntese calcula resistencia
//  Objetivo:   toma valores e calcula resist.
//  Entrada : cada resistencia
//  Saída   : resistencias series/parlelo


principal
	// Declarações
	
	real resistencias[7];
	inteiro i;
	real x;
	i = 0;
	// Instruções
	faca
		leia(x);
		se(x >0) entao
			resistencias[i] = x;
			i = i+1;
		fimSe
	enquanto (x >0); 
	

	escreval("VALOR EM SERIE = ", resistencia_serie(resistencias, i)::2);
	escreval("VALOR EM PARALELO = ", resistencia_paralelo(resistencias, i)::4);

	

fimPrincipal
funcao real resistencia_serie(real vetor[], inteiro tamanho_vetor)
	real resistencia_s;
	inteiro i;
	resistencia_s = 0;
	para (i de 0 ate tamanho_vetor-1 passo 1) faca
		resistencia_s = resistencia_s + vetor[i];
	fimPara
	retorna resistencia_s;
fimFuncao
funcao real resistencia_paralelo(real vetor[], inteiro tamanho_vetor)
	inteiro i;
	real resistencia_p;
	real soma;
	soma = 0;
	para (i de 0 ate tamanho_vetor-1 passo 1) faca
		soma = soma + 1/vetor[i];
	fimPara
	
	resistencia_p = exp(soma, -1);
	retorna resistencia_p;
fimFuncao
