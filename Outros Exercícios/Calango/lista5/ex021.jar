algoritmo resistencia;
// Síntese calcula resistencia
//  Objetivo:   toma valores e calcula resist.
//  Entrada : cada resistencia
//  Saída   : resistencias series/parlelo


principal
	real resistencias[100];


	inteiro i;
	inteiro x;
	i = 0;
	x = 1;
	enquanto (x!=0) faca
		leia(x);
		resistencias[i] = x;
		i = i + 1;
	fimEnquanto

	escreval("VALOR EM SERIE = ", resistencia_serie(resistencias, i+1)::2);
	escreval("VALOR EM PARALELO = ", resistencia_paralelo(resistencias, i+1)::4);
fimPrincipal

funcao real resistencia_serie(real vetor[], inteiro N)
	inteiro i;
	real res_s;
	res_s = 0;

	para (i de 0 ate N-1 passo 1) faca
		res_s = res_s + vetor[i];
	fimPara

	retorna res_s;
fimFuncao

funcao real resistencia_paralelo(real vetor[], inteiro N)
	inteiro i;
	real res_p, soma;
	soma = 0;
	para (i de 0 ate N-1 passo 1) faca
		soma = soma + 1/vetor[i];
	fimPara
	res_p = exp(soma, -1);
	retorna res_p;
fimFuncao
