algoritmo resistencia;
// Síntese calcula resistencia
//  Objetivo:   toma valores e calcula resist.
//  Entrada : cada resistencia
//  Saída   : resistencias series/parlelo


principal
	real res_serie, res_paralelo;


	inteiro x;
	x = 0;
	enquanto (x<=0) faca
		leia(x);
		res_serie = res_serie + x;
		res_paralelo = 1/(1/res_paralelo+1/x);
	fimEnquanto

	escreval("VALOR EM SERIE = ", res_serie::2);
	escreval("VALOR EM PARALELO = ", res_paralelo::4):

fimPrincipal
